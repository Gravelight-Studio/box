{
  "name": "@gravelight/box",
  "version": "0.2.0",
  "description": "Annotation-driven API framework with hybrid cloud deployment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "sync-version": "node scripts/sync-version.js",
    "prepublishOnly": "npm run sync-version && npm run build",
    "publish:package": "npm publish",
    "publish:dry-run": "npm publish --dry-run"
  },
  "keywords": [
    "api",
    "framework",
    "annotations",
    "cloud",
    "serverless",
    "gcp",
    "cloud-functions",
    "cloud-run",
    "hybrid-cloud",
    "typescript"
  ],
  "author": "Gravelight Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gravelight-studio/box.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/gravelight-studio/box/issues"
  },
  "homepage": "https://github.com/gravelight-studio/box#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "pino": "^10.1.0",
    "pino-pretty": "^10.3.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
